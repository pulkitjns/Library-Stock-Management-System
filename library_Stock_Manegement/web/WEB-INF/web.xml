<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>registeration</servlet-name>
        <servlet-class>registeration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Singleitem</servlet-name>
        <servlet-class>Singleitem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bulkitem</servlet-name>
        <servlet-class>Bulkitem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Searchmag</servlet-name>
        <servlet-class>Searchmag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>magentry</servlet-name>
        <servlet-class>magentry</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registeration</servlet-name>
        <url-pattern>/registeration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Singleitem</servlet-name>
        <url-pattern>/Singleitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bulkitem</servlet-name>
        <url-pattern>/Bulkitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Searchmag</servlet-name>
        <url-pattern>/Searchmag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>magentry</servlet-name>
        <url-pattern>/magentry</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
